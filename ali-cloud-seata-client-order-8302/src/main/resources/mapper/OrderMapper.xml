<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ali.cloud.seata.mapper.OrderMapper">
    <resultMap id="order" type="com.ali.cloud.seata.entity.OrderDTO">
        <result column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="commodity_code" property="commodityCode"/>
        <result column="count" property="count"/>
        <result column="money" property="money"/>
    </resultMap>

    <insert id="createOrder" keyProperty="id" useGeneratedKeys="true" parameterType="com.ali.cloud.seata.entity.OrderDTO">
        insert into order_tbl values (null, #{userId}, #{commodityCode}, #{count}, #{money})

    </insert>
</mapper>